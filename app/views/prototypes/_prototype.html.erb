<div class="card">
  <%= link_to image_tag("デザインカンプ（一覧表示画面）.png", class: :card__img), prototype_path(prototype.id) %>
  <div class="card__body">
    <%= link_to prototype.title, root_path, class: :card__title%>
    <p class="card__summary">
      <%= prototype.catch_copy %>
    </p>
    <%= link_to "by #{current_user.name}", root_path, class: :card__user %>
  </div>
</div>



